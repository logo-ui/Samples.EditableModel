<UserControl x:Class="LogoUI.Samples.Client.Gui.Modules.Automation.Views.AutomationRootView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:viewModels="clr-namespace:LogoUI.Samples.Client.Gui.Modules.Automation.ViewModels"
             xmlns:cal="http://www.caliburnproject.org"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance viewModels:AutomationRootViewModel}"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ContentControl cal:View.Model="{Binding Path=MainViewModel, Mode=OneTime}" />  
    </Grid>
</UserControl>
